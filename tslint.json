{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-implicit-dependencies": [true, ["@testing-library/react"]],
    "jsx-no-lambda": false,
    "no-submodule-imports": false,
    "no-console": [true, "log"]
  }
}
