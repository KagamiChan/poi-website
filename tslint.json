{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-implicit-dependencies": [true, ["@testing-library/react", "@testing-library/dom"]],
    "jsx-no-lambda": false,
    "no-submodule-imports": false,
    "no-console": [true, "log"]
  },
  "linterOptions": {
    "exclude": [
        "node_modules/**"
    ]
  }
}
